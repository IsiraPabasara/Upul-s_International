
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id          String      @id @default(auto()) @map("_id") @db.ObjectId
  firstname   String
  lastname    String
  email       String      @unique
  phonenumber String      @unique
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}